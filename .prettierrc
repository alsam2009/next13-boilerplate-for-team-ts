{
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "proseWrap": "preserve",
  "singleAttributePerLine": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/layout/(.*)$",
    "^@/ui/(.*)$",
    "^@/providers/(.*)$",
    "^@/db/(.*)$",
    "^@/store/(.*)$",
    "^@/constants/(.*)$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/assets/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "^(?!.*[.]scss$)[./].*$",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}